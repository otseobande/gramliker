{
	"manifest_version": 2,

	"name" : "Instaliker",
	"description": "Smart Instagram Bot",
	"version" : "1.0",

	"browser_action":{
		"default_popup": "./html/popup.html"
	},

	"permissions":[
		"tabs",
		"cookies",
		"*://*.instagram.com/",
		"alarms",
	    "background",
	    "power",
	    "storage",
	    "unlimitedStorage",
	    "webRequest",
	    "webRequestBlocking",
		"*://*.instagram.com/"
	]
}